{
  "expo": {
    "name": "QuickMart",
    "slug": "quickmart-user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "package": "com.rajeshn67.quickmartuser",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAKZpS23x4O9g1Rl8MAWdDZAjKuEqqIkQU"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-location",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow QuickMart to use your location to show nearby stores and delivery options."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "33f33eb3-2d93-430a-943f-6ebab3f1ab14"
      }
    }
  }
}
